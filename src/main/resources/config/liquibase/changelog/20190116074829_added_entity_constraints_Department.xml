<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Department.
    -->
    <changeSet id="20190116074829-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="student_id"
                                 baseTableName="department"
                                 constraintName="fk_department_student_id"
                                 referencedColumnNames="id"
                                 referencedTableName="student"/>

        <addForeignKeyConstraint baseColumnNames="branch_id"
                                 baseTableName="department"
                                 constraintName="fk_department_branch_id"
                                 referencedColumnNames="id"
                                 referencedTableName="branch"/>

        <addForeignKeyConstraint baseColumnNames="academicyear_id"
                                 baseTableName="department"
                                 constraintName="fk_department_academicyear_id"
                                 referencedColumnNames="id"
                                 referencedTableName="academic_year"/>

    </changeSet>
</databaseChangeLog>
